
--Creating  view for Aw_Calendar_df
CREATE VIEW Aw_Calendar
AS
SELECT
     *
FROM
    OPENROWSET(
        BULK 'https://adventureprojectuday.dfs.core.windows.net/silver/Aw_Calendar_df',
        FORMAT = 'PARQUET'
    ) AS result1

